from mrjob.job import MRJob
import numpy as np

class EmpleadosPromSE(MRJob):

    def mapper(self, _, line):

	empleado = line.split(",")
	yield empleado[1], int(empleado[0])

    def reducer(self, key, values):
	cont = 0
        lista = list(values)
        mset = set(lista)
        for v in mset:
            cont+=1
	yield key, len(mset)

if __name__ == '__main__':
    EmpleadosPromSE.run()

